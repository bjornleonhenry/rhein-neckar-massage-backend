{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "axyr/laravel-react-starter-kit",
    "type": "project",
    "description": "Laravel React Starter Kit without Inertia.",
    "keywords": [
        "laravel",
        "framework",
        "starter kit",
        "react"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-quick-create": "^4.0",
        "bezhansalleh/filament-language-switch": "^4.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "cheesegrits/filament-google-maps": "^3.0",
        "coolsam/modules": "^5.0",
        "filaforge/filament-api-explorer": "^1.0",
        "filaforge/filament-database-viewer": "^1.0",
        "filaforge/filament-user-manager": "^1.0",
        "filament/actions": "^4.0",
        "filament/filament": "^4.0",
        "filament/forms": "^4.0",
        "filament/infolists": "^4.0",
        "filament/notifications": "^4.0",
        "filament/schemas": "^4.0",
        "filament/spatie-laravel-settings-plugin": "^4.0",
        "filament/tables": "^4.0",
        "filament/widgets": "^4.0",
        "hugomyb/filament-media-action": "^4.0",
        "inerba/filament-db-config": "^1.1",
        "laravel/fortify": "^1.26",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.1",
        "laravel/telescope": "^5.11",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6.4",
        "marjose123/filament-webhook-server": "^3.0",
        "solution-forest/filament-tree": "^3.0",
        "spatie/laravel-package-tools": "*",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-webhook-server": "*",
        "stechstudio/filament-impersonate": "^4.0",
        "swisnl/filament-backgrounds": "^2.0",
        "zpmlabs/filament-api-docs-builder": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi",
            "@php artisan frontend:install --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve --host 0.0.0.0\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"cd ../frontend && npm run dev -- --host 0.0.0.0\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            
            ]
        },
        "merge-plugin": {
            "merge-extra": true,
            "merge-extra-deep": true,
            "include": [
                "app-modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
